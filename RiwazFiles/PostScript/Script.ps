%Draw a box

/bx {
newpath
2 copy
moveto
1 0 rlineto
0 1 rlineto
-1 0 rlineto
0 -1 rlineto
closepath
} def

%Draw and paint A

/A {
bx
color1 setrgbcolor
fill
bx 1 setgray
stroke
spc
} def

%Draw and paint B

/B {
bx
color2 setrgbcolor
fill
bx 1 setgray
stroke
spc
} def

%Leave a space

/spc {
exch 1 add exch
} def

%Move to new line

/nl {
1 add
exch
pop
0 exch
} def

